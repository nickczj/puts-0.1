{
  
	"manifest_version": 2,

  

	"name": "PUTS",
  
	"description": "Shopping extension",
  
	"version": "1.0",
	
	"options_ui":{
		"page": "options.html",
		"chrome_style": true
		},
		
	"permissions": [
		"storage",
		"unlimitedStorage",
		"tabs"
		],
  

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "Add to list"

	},
  
	"permissions": [
    
		"activeTab",
    
	"https://ajax.googleapis.com/"
  
	]

}
